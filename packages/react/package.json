{
  "name": "@yaasl/react",
  "version": "0.0.1",
  "description": "React package of yaasl",
  "keywords": [
    "react",
    "atom",
    "store"
  ],
  "author": "PrettyCoffee",
  "license": "MIT",
  "exports": {
    "import": {
      "default": "./dist/mjs/index.js",
      "types": "./dist/@types/index.d.ts"
    },
    "require": {
      "default": "./dist/cjs/index.js",
      "types": "./dist/@types/index.d.ts"
    }
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/@types/index.d.ts",
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf ./dist",
    "compile": "run-p compile:mjs compile:cjs compile:types",
    "compile:mjs": "tsc --p ./tsconfig.mjs.json",
    "compile:cjs": "tsc --p ./tsconfig.cjs.json",
    "compile:types": "tsc --p ./tsconfig.mjs.json --declaration --emitDeclarationOnly --outDir ./dist/@types",
    "test": "jest --testMatch **/react/**/*.test.* --config ../../jest.config.json",
    "test:watch": "npm run test -- --watchAll"
  },
  "peerDependencies": {
    "react": ">=17",
    "@yaasl/core": ">= 0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.6",
    "@yaasl/core": "file:../core",
    "react": "^18.2.0"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc",
      "@pretty-cozy/eslint-config/react"
    ]
  }
}
